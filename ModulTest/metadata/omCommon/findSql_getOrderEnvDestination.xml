<?xml version="1.0" encoding="UTF-8" ?>
<findSql name="omCommon.getOrderEnvDestination">
  <dbSchema>OM_COMMON</dbSchema>
  <extends>find_com.conceptwave.system.Finder</extends>
  <input>doc_omCommon.omOrderEnvSearchDoc</input>
  <label>Get Environment Config By Order Id</label>
  <metaVersion>21</metaVersion>
  <output>doc_omCommon.omEnvironmentConfig</output>
  <type>SQL</type>
  <volatility>0</volatility>
  <methodList>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        SELECT {+*+} FROM OM_ENVIRONMENT_CONFIG
        WHERE ACTIVE = 1 and ENVIRONMENT IN (SELECT ENVIRONMENT FROM OM_DEPENDENTS_ORDER WHERE 1 = 1
        {[0] and DESTINATIONORDERID = ?})
        {[1] and environmentType = ?}
      ]]></script>
    </method>
  </methodList>
  <UserInterface name="UserInterface" type="ui">
    <extends>ui_com.conceptwave.system.FinderUserInterface</extends>
    <variableList>
      <variable name="result" type="uivar">
        <flags enum-type="metadataFlag">ARRAY</flags>
        <valueType>doc_omCommon.omEnvironmentConfig</valueType>
      </variable>
      <variable name="detail" type="uivar">
        <flags enum-type="metadataFlag">VALIDATE</flags>
        <valueType>doc_omCommon.omEnvironmentConfig/ui_UserInterface</valueType>
      </variable>
      <variable name="search" type="uivar">
        <valueType>doc_omCommon.omOrderEnvSearchDoc/ui_UserInterface</valueType>
      </variable>
    </variableList>
    <vformList>
