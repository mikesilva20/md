  <methodList>
    <method name="cwOnDocStore" type="action">
      <system>true</system>
      <script><![CDATA[
        this.lastUpdateDate = new Date();
        this.userId = UserProfile.userId();

        var dbObj = Document.readDoc("omMessaging.messagingConfiguration", "activeConfig");

        var oldXML = "n/a";

        if(dbObj){
            oldXML = dbObj.toXML();
        }


        this.cw$super_cwOnDocStore();


        omMessaging.cache_Loader(this.getCacheType());


        Global.showUserMessage("A configuracao foi alterada com sucesso!");
      ]]></script>
    </method>
