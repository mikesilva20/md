  <methodList>
    <method name="cwOnFinderCompleteSearch" type="action">
      <system>true</system>
      <script>omMessaging.showMsgHierarchy(this);</script>
    </method>
    <method name="cwOnFinderSQLSel" type="action">
      <system>true</system>
      <script><![CDATA[
        select npu, correlationNpu, businessId, status, statusCode, statusText, system, operation, processId, interfaceToInvoke, CREATIONDATE,  metadataType, inbound
        FROM
        (      (
                    select npu, correlationNpu, businessId, status, statusCode, statusText, system, operation, processId, interfaceToInvoke, CREATIONDATE, 'omMessaging:inboundDoc' metadataType, 1 inbound, stackId
                    FROM OM_MSG_INBOUND
                    WHERE 1=1
                          AND ROWNUM <= 1000
                     {[0] AND npu = ?}
                     {[1] AND correlationNpu = ?}
                     {[2] AND businessId = ?}
                     {[4] AND STATUS = ?}
                     {[9] AND processId = ?}
                     {[7] AND CREATIONDATE >= ?}
                     {[8] AND CREATIONDATE <= ?}
                     {[10] AND stackId = ?}
              )
          UNION ALL
                (
                    select npu, correlationNpu, businessId, status, statusCode, statusText, system, operation, processId, interfaceToInvoke, CREATIONDATE, 'omMessaging:archiveInboundDoc' metadataType, 1 inbound, stackId
                    FROM OM_MSG_INBOUND_ARCHIVE
                    WHERE 1=1
                          AND ROWNUM <= 1000
                     {[0] AND npu = ?}
                     {[1] AND correlationNpu = ?}
                     {[2] AND businessId = ?}
                     {[4] AND STATUS = ?}
                     {[9] AND processId = ?}
                     {[7] AND CREATIONDATE >= ?}
                     {[8] AND CREATIONDATE <= ?}
                     {[10] AND stackId = ?}
              )
                UNION ALL

              (
                    select npu, correlationNpu, businessId, status, statusCode, statusText, system, operation, processId, interfaceToInvoke, CREATIONDATE, 'omMessaging:outboundDoc' metadataType, 0 inbound, stackId
                    FROM OM_MSG_OUTBOUND
                    WHERE 1=1
                          AND ROWNUM <= 1000
                    {[0] AND npu = ?}
                    {[1] AND correlationNpu = ?}
                    {[2] AND businessId = ?}
                    {[4] AND STATUS = ?}
                    {[9] AND processId = ?}
                    {[7] AND CREATIONDATE >= ?}
                    {[8] AND CREATIONDATE <= ?}
                     {[10] AND stackId = ?}
            )
              UNION ALL
              (
                    select npu, correlationNpu, businessId, status, statusCode, statusText, system, operation, processId, interfaceToInvoke, CREATIONDATE, 'omMessaging:archiveOutboundDoc' metadataType, 0 inbound, stackId
                    FROM OM_MSG_OUTBOUND_ARCHIVE
                    WHERE 1=1
                          AND ROWNUM <= 1000
                    {[0] AND npu = ?}
                    {[1] AND correlationNpu = ?}
                    {[2] AND businessId = ?}
                    {[4] AND STATUS = ?}
                    {[9] AND processId = ?}
                    {[7] AND CREATIONDATE >= ?}
                    {[8] AND CREATIONDATE <= ?}
                    {[10] AND stackId = ?}
              )
        )
        WHERE 1=1
        AND ROWNUM <= 2000
        {[5] AND (NPU NOT like '0022INOTIF_%' AND NPU NOT like '0022INTNOTIFIC%') }
        {[7] AND CREATIONDATE >= ?}
        {[8] AND CREATIONDATE <= ?}
        ORDER BY CREATIONDATE DESC
      ]]></script>
    </method>
