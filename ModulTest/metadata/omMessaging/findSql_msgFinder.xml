      <method name="sentData" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <script><![CDATA[
          var selectList = this.result.selected;

          var selected = (selectList ? selectList[0] : null);
          if(selected){

                var msg =  Document.readDoc(selected.metadataType, selected.npu);

                omMessaging.aux_loadMsgData(msg);

                var data = null;

                if(selected.inbound)
                      data = msg.receivedData;
                else
                      data = msg.payload;

                if(data)
                      Global.showModelessDialog(data,700,560, "text/xml");
                else
                      Global.showUserMessage("This container is empty.");


          }
        ]]></script>
