      <method name="generateProject" type="usrActionMethod">
        <allowInvalidObject>true</allowInvalidObject>
        <autosave>true</autosave>
        <dialogHeight>600</dialogHeight>
        <dialogWidth>800</dialogWidth>
        <parameterList>
          <parameter name="cwDialogParam" type="rifp">
            <type>ui_com.conceptwave.system.Dialog</type>
          </parameter>
        </parameterList>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          cwDialogParam.title = Global.getElementLabel("omMessaging.msgFinder.UserInterface.Forms.Menu/finderMenuLayout.generateProject", "Generate Project");

          var finder = this.model;
          // get the project SOAP for this.model order

          if(finder.searchDocument.businessId){

                var soapXML = soapUI.genProjectSOAPV2(finder.searchDocument.businessId);
                var mngmDoc = new Document("soapUI:testCaseManagement");

                mngmDoc.projectName = finder.searchDocument.businessId;
                mngmDoc.soapCase    = soapXML;

                return mngmDoc;

          }
        ]]></script>
      </method>
